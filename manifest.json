{
  "manifest_version": 2,
  "name": "Privacy friendly Cookie settings",
  "version": "1.0",
  "description": "Dieses Plugin ermöglicht es Ihnen auf einfache weise Ihre Einstellungen, bezüglich der verwaltung der Cookies, vorzunehmen.",
  "icons": {
    "16": "img/logo16.png",
    "48": "img/logo48.png",
    "128": "img/logo128.png"
  },
  "permissions": [
    "history",
    "nativeMessaging",
    "contentSettings",
    "privacy",
    "browsingData",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "img/logo16.png",
      "24": "img/logo24.png",
      "32": "img/logo32.png"
    },
    "default_title": "Cookie Settings",
    "default_popup": "popup.html"
  },
  "author": "Oliver Ernst Käfer",
  "options_page": "options.html"
}